{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "hide-files.files": [],
    "pylint.cwd": "${workspaceFolder}/app",
    "todo-tree.tree.scanMode": "workspace only",
    "todo-tree.tree.showCountsInTree": true,
    "todo-tree.tree.disableCompactFolders": true,
    "todo-tree.tree.showBadges": true,
    "cSpell.words": [
        "ABAC",
        "APIKEY",
        "Authy",
        "Bluesky",
        "cachetools",
        "changeme",
        "itsdangerous",
        "joinedload",
        "jsonable",
        "loguru",
        "mailjet",
        "psycopg",
        "pyotp",
        "qrcode",
        "repr",
        "sessionmaker",
        "sessionmanager",
        "Tesseract",
        "unban"
    ],
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "database": "${workspaceFolder:UserManagementAPI_DEV}/data/Project.db",
            "name": "Project"
        }
    ],
    "sqltools.useNodeRuntime": true
}