{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "hide-files.files": [],
    "pylint.cwd": "${workspaceFolder}/app",
    "todo-tree.tree.scanMode": "workspace only",
    "todo-tree.tree.showCountsInTree": true,
    "todo-tree.tree.disableCompactFolders": true,
    "todo-tree.tree.showBadges": true,
    "cSpell.words": [
        "APIKEY",
        "cachetools",
        "itsdangerous",
        "loguru",
        "mailjet",
        "psycopg",
        "pyotp",
        "qrcode",
        "repr",
        "Tesseract",
        "unban"
    ]
}